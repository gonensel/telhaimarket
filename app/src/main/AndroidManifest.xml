<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.telhaimarket">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Telhaimarket"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".ResetPassword"
            android:exported="false" />
        <activity
            android:name=".ProfileEditPage"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:exported="true"
            android:label="@string/title_activity_profile_page"
            android:theme="@style/Theme.Telhaimarket.Fullscreen" />
        <activity
            android:name=".ProfilePage"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:exported="true"
            android:label="@string/title_activity_profile_page"
            android:theme="@style/Theme.Telhaimarket.Fullscreen" />
        <activity
            android:name=".NewPost"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:exported="true"
            android:label="@string/title_activity_new_post"
            android:theme="@style/Theme.Telhaimarket.Fullscreen" />
        <activity
            android:name=".Register"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:exported="true"
            android:label="@string/title_activity_register"
            android:theme="@style/Theme.Telhaimarket.Fullscreen" />
        <activity
            android:name=".Login"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:exported="true"
            android:label="@string/title_activity_login"
            android:theme="@style/Theme.Telhaimarket.Fullscreen"></activity>
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>